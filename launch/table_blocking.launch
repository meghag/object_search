<launch>

  <!-- spawn table and tabletop object-->
  <param name="table_description" 
   command="$(find xacro)/xacro.py '$(find manipulation_worlds)/urdf/table.urdf.xacro'" />
  <node name="spawn_table" pkg="gazebo" type="spawn_model" args="-urdf -param table_description -model table_1" 
   respawn="false" output="screen" />
   
   <!--node name="spawn_object3" pkg="gazebo" type="spawn_model" 
   args="-urdf -file $(find tum_os)/objects/green_box_tuned.urdf -model green_box -x 0.7 -y -0.2 -z 0.555" 
   respawn="false" output="screen" /-->

   <node name="spawn_object4" pkg="gazebo" type="spawn_model" 
   args="-urdf -file $(find tum_os)/objects/blue_box_tuned.urdf -model blue_box -x 0.7 -y -0.1 -z 0.555" 
   respawn="false" output="screen" />

   <node name="spawn_object5" pkg="gazebo" type="spawn_model" 
   args="-urdf -file $(find tum_os)/objects/small_box_tuned.urdf -model small_box -x 0.55 -y -0.1 -z 0.555" 
   respawn="false" output="screen" />

   <node name="spawn_object6" pkg="gazebo" type="spawn_model" 
   args="-urdf -file $(find tum_os)/objects/small_box_tuned_smaller.urdf -model smaller_box -x 0.55 -y .2 -z 0.555" 
   respawn="false" output="screen" />

  <!-- parameters that give you reasonable physics as well as good speed -->
  <!-- needed so that gripper joint stops on objects instead of oscillating forever -->
  <node name="sor_pgs_iters" pkg="dynamic_reconfigure" type="dynparam" args="set gazebo sor_pgs_iters 50"/>
  <node name="sor_pgs_w" pkg="dynamic_reconfigure" type="dynparam" args="set gazebo sor_pgs_w 1.0"/>
  <node name="time_step" pkg="dynamic_reconfigure" type="dynparam" args="set gazebo time_step 0.001"/>

  <!--
  <node name="move_pr2" pkg="pr2_gazebo" type="move_pr2"/>
  -->

</launch>
